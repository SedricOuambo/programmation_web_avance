{
  "compilerOptions": {
    "types": [
      // Other types...
      // This allows Serwist to type `window.serwist`.
      "@serwist/next/typings"
    ],
    "lib": ["dom", "dom.iterable", "esnext", "webworker"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/NavBar.jsx", "components/Slidebar.jsx", "components/BlocModule.jsx", "components/IndexProvider.jsx", "components/MonSousBloc.jsx", "components/MonBloc.jsx", "components/MonBloc.jsx", "app/module1/page.jsx", "components/Accueil.jsx", "components/ModuleAccueil.jsx"],
  "exclude": ["node_modules"]
}
